{
  "expo": {
    "name": "VoiceAgent",
    "slug": "voiceagent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "voiceagent",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voiceagent.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs microphone access for voice conversations with AI agents."
      }
    },
    "android": {
      "package": "com.voiceagent.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b05cfcf2-4aa0-49ac-9b1f-bc2488b703af"
      }
    }
  }
}
